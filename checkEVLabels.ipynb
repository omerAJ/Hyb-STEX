    "import torch\n",
    "\n",
    "# Create a torch tensor\n",
    "tensor = torch.tensor([1, 2, 3, 4, 5])\n",
    "\n",
    "# Create a mask using torch.ge\n",
    "mask = torch.ge(tensor, 3)\n",
    "\n",
    "# Use masked_select on the tensor\n",
    "masked_tensor = torch.masked_select(tensor, ~mask)\n",
    "\n",
    "print(masked_tensor)"
   ]
  }
